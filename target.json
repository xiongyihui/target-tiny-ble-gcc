{
  "name": "tiny-ble-gcc",
  "version": "1.0.0",
  "inherits": {
    "nordic-nrf51822-gcc": "^1.0.0"
  },
  "description": "mbed build target for the Seeed Tiny BLE (16K RAM nRF51822) platform.",
  "homepage": "https://github.com/xiongyihui/target-tiny-ble-gcc",
  "repository": {
    "url": "https://github.com/xiongyihui/target-tiny-ble-gcc",
    "type": "git"
  },
  "licenses": [
    {
      "url": "https://spdx.org/licenses/Apache-2.0",
      "type": "Apache-2.0"
    }
  ],
  "keywords": [
    "mbed-official",
    "mbed-target:tiny_ble",
    "gcc"
  ],
  "similarTo": [
    "nrf51dk"
  ],
  "config": {
    "nrf51822": {
      "ram_size": "16K"
    },
    "image": {
      "heap": {
        "warning_threshold": 1024
      }
    },
    "hardware": {
      "pins": {
        "LED1": "p21",
        "LED2": "p22",
        "LED3": "p23",
        "BUTTON1": "p17",
        "RX_PIN_NUMBER": "p11",
        "TX_PIN_NUMBER": "p9",
        "CTS_PIN_NUMBER": "p10",
        "RTS_PIN_NUMBER": "p8",
        "USBTX": "TX_PIN_NUMBER",
        "USBRX": "RX_PIN_NUMBER",
        "I2C_SDA0": "p18",
        "I2C_SCL0": "p19",
        "MPU6050_SDA_PIN": "p18",
        "MPU6050_SCL_PIN": "p19",
        "MPU6050_INT_PIN": "p20",
        "A0": "p1",
        "A1": "p2",
        "A2": "p3",
        "A3": "p4",
        "A4": "p5",
        "A5": "p6"
      }
    }
  }
}
